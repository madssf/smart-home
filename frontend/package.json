    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "cross-env": "7.0.3",
    "dayjs": "^1.11.5",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "firebaseui": "^6.0.1",
    "framer-motion": "^6.5.1",
    "jwt-decode": "^3.1.2",
